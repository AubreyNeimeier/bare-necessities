<h1><%= @user.username %>'s Calendar will be Here! </h1>
<hr>
<hr>

<%= form_for(@event) do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %><br>
    <%= f.label :description %>
    <%= f.text_field :description %><br>
    <%= f.label :date %>
    <%= f.text_field :date %><br>
    <%= f.label :start_time %>
    <%= f.text_field :start_time %><br>
    <%= f.label :end_time %>
    <%= f.text_field :end_time %><br>

    <%=f.submit "Create Event"%>
<% end %>
